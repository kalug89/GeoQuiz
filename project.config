[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/kalug89
[access "refs/for/refs/*"]
	read = group user/kalug89
	push = group user/kalug89
	label-Code-Review = -2..+2 group user/kalug89
	label-Verified = -1..+1 group user/kalug89
	rebase = group user/kalug89
	removeReviewer = group user/kalug89
	submit = group user/kalug89
[access "refs/heads/*"]
	read = group user/kalug89
	create = group user/kalug89
	push = +force group user/kalug89
	pushMerge = group user/kalug89
[access "refs/tags/*"]
	push = group user/kalug89
	push = +force group user/kalug89
